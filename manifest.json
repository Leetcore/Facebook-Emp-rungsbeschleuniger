{
  "manifest_version": 2,

  "name": "Empörungsbeschleuniger",
  "description": "Lässt euch schneller ein Empörungspost auf Facebook.com veröffentlichen!",
  "version": "1.2",
  "content_scripts": [
  {
   "matches": ["https://*.facebook.com/*", "http://*.facebook.com/*"],
   "js": ["empoerung.js"],
   "run_at": "document_end"
  }
 ],
  "permissions": [
    "https://*.facebook.com/*", "http://*.facebook.com/*"
  ]
}
